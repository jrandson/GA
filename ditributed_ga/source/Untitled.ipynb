    "import matplotlib as mpl\n",
    "from matplotlib import pyplot as plt\n",
    "from math import sqrt,sin\n",
    "\n",
    "eps = -120\n",
    "x = range(100)\n",
    "\n",
    "#print x\n",
    "r = [sqrt(i**2) + eps for i in x]\n",
    "y = [sin(i)/i for i in r]\n",
    "\n",
    "print(min(y))\n",
    "\n",
    "plt.plot(x,y,'r*')\n",
    "plt.show()\n",
    "\n",
